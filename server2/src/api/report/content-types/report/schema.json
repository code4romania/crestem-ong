{
  "kind": "collectionType",
  "collectionName": "reports",
  "info": {
    "singularName": "report",
    "pluralName": "reports",
    "displayName": "Report"
  },
  "options": {
    "comment": ""
  },
  "attributes": {
    "evaluations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::evaluation.evaluation",
      "mappedBy": "report"
    },
    "deadline": {
      "type": "date",
      "required": true
    },
    "finished": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "reports"
    }
  }
}
